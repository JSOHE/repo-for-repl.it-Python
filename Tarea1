a = ['perro','gato','pollo']
a.append('conejo')
print('De', len(a), 'animales el', a[len(a)-1], 'fue el último')

def par_impar(a):
  if a <= 1:
      poim = 'inválido, escribe un número mayor a 1'
  elif (a % 2) == 1:
        poim = 'impar'
  else:
        poim = 'par'
  return poim      
a = 3598
print(a, 'es número ' + par_impar(a))